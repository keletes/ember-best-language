{
  "name": "ember-best-language",
  "version": "1.0.3",
  "description": "An FastBoot-enabled addon to detect the best language for your user.",
  "keywords": [
    "ember-addon",
    "language",
    "best",
    "locale",
    "fastboot"
  ],
  "license": "BSD-3-Clause",
  "author": "Charles Demers <cdemers@mirego.com>",
  "contributors": [
    "Pascal Boutin <pboutin@mirego.com>"
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/mirego/ember-best-language",
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "ember test",
    "test:all": "ember try:each",
    "prettier": "prettier --write '{addon,app,tests,config}/**/*.{ts,js}' ember-cli-build.js testem.js",
    "prettier-check": "prettier -l '{addon,app,tests,config}/**/*.{ts,js}' ember-cli-build.js testem.js",
    "lint": "node_modules/.bin/eslint '{addon,app,config,tests}/**/*.js' './index.js' ember-cli-build.js testem.js",
    "lint-typescript": "tslint -c tslint.json '{addon,app,config,tests,config}/**/*.ts'",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean"
  },
  "dependencies": {
    "@ember-decorators/babel-transforms": "^3.1.0",
    "ember-cli-babel": "^7.1.0",
    "ember-decorators": "^3.1.4"
  },
  "devDependencies": {
    "@ember/optional-features": "0.7.0",
    "@types/ember": "^3.0.25",
    "@types/ember-mocha": "^0.14.5",
    "@types/ember-test-helpers": "^1.0.4",
    "@types/ember-testing-helpers": "^0.0.3",
    "@types/ember__test-helpers": "^0.7.6",
    "@types/mocha": "^5.2.5",
    "@types/rsvp": "^4.0.2",
    "babel-eslint": "^8.0.0",
    "broccoli-asset-rev": "3.0.0",
    "ember-cli": "3.5.1",
    "ember-cli-chai": "0.5.0",
    "ember-cli-dependency-checker": "3.0.0",
    "ember-cli-fastboot": "2.0.0",
    "ember-cli-htmlbars": "3.0.1",
    "ember-cli-htmlbars-inline-precompile": "2.0.0",
    "ember-cli-inject-live-reload": "2.0.1",
    "ember-cli-mocha": "0.15.0",
    "ember-cli-shims": "1.2.0",
    "ember-cli-sri": "2.1.1",
    "ember-cli-typescript": "^1.5.0",
    "ember-cli-uglify": "2.1.0",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-load-initializers": "2.0.0",
    "ember-mocha": "0.14.0",
    "ember-resolver": "5.0.1",
    "ember-source": "3.5.1",
    "ember-source-channel-url": "1.1.0",
    "ember-try": "1.1.0",
    "eslint": "5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-ember": "^6.0.1",
    "eslint-plugin-mirego": "0.0.1",
    "loader.js": "4.7.0",
    "prettier": "^1.15.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.1"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*",
    "npm": "6.4.1"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
