{
  "name": "ember-best-language",
  "version": "2.0.3",
  "description": "A FastBoot-enabled addon to detect the best language for your user.",
  "keywords": [
    "ember-addon",
    "language",
    "best",
    "locale",
    "fastboot"
  ],
  "license": "BSD-3-Clause",
  "author": "Charles Demers <cdemers@mirego.com>",
  "contributors": [
    "Pascal Boutin <pboutin@mirego.com>"
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/mirego/ember-best-language",
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "ember test",
    "test:all": "ember try:each",
    "prettier": "prettier --write '{addon,app,tests,config}/**/*.{ts,js}' ember-cli-build.js testem.js",
    "prettier-check": "prettier -l '{addon,app,tests,config}/**/*.{ts,js}' ember-cli-build.js testem.js",
    "lint": "npx eslint --ext .js,.ts .",
    "type-check": "npx tsc",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean"
  },
  "dependencies": {
    "ember-cli-babel": "^7.12.0",
    "ember-cli-typescript": "^4.2.1"
  },
  "devDependencies": {
    "@ember/optional-features": "2.0.0",
    "@types/chai": "4.2.22",
    "@types/ember": "3.16.5",
    "@types/ember-mocha": "0.14.12",
    "@types/ember-test-helpers": "1.0.10",
    "@types/ember-testing-helpers": "0.0.4",
    "@types/ember__test-helpers": "2.0.2",
    "@types/mocha": "9.0.0",
    "@types/rsvp": "4.0.4",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "babel-eslint": "^10.0.3",
    "broccoli-asset-rev": "3.0.0",
    "chai-dom": "^1.0.0",
    "ember-cli": "3.28.4",
    "ember-cli-chai": "^0.5.0",
    "ember-cli-dependency-checker": "3.2.0",
    "ember-cli-fastboot": "2.2.3",
    "ember-cli-htmlbars": "6.0.0",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-sri": "2.1.1",
    "ember-cli-typescript-blueprints": "3.0.0",
    "ember-cli-uglify": "3.0.0",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-load-initializers": "2.1.2",
    "ember-mocha": "0.16.2",
    "ember-native-dom-helpers": "0.7.0",
    "ember-resolver": "8.0.3",
    "ember-source": "3.28.6",
    "ember-source-channel-url": "3.0.0",
    "ember-try": "2.0.0",
    "eslint": "8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.8",
    "eslint-plugin-mirego": "1.0.0",
    "eslint-plugin-node": "11.1.0",
    "loader.js": "4.7.0",
    "prettier": "^2.4.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "4.5.2"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*",
    "yarn": ">= 1.16.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
